#ifndef Vector_h
#define Vector_h

class Vector{
	public:
		double x,y;
		Vector(){};
		Vector::Vector(double a, double b){
			x=a;
			y=b;
		}

		void Vector::setX(double X0){
			x=X0;
		}

		void Vector::setY(double Y0){
			y=Y0;
		}

		double Vector::X(){
			return x;
		}

		double Vector::Y(){
			return y;
		}

		Vector Vector::operator*(Vector param){
			Vector temp;

			return temp;
		}
};

#endif
